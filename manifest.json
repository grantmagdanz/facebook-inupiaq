{
"manifest_version": 2,
    "name": "Facebook in Iñupiaq",
    "description": "Translates Facebook's interface into Iñupiaq.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*.facebook.com/*"
            ],
            "js": [
                "content.user.js"
                
            ],
            "run_at": "document_end"
        }
    ]
}